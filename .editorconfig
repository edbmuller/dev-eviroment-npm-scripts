# To enable this file go to editorconfig.org, click on the editor of yout choice to find the right extension and install it. Done!
root = true

[*]
indent_style = tab
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
